<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="cn.com.icore.user.model.SysUserAndSysGroupToSysRole"
		table="SYSUSERANDGROUPTOROLE">
		<id name="id" type="java.lang.Long" column="id">
			<generator class="assigned" />
		</id>



		<discriminator column="mark" type="java.lang.String" />


		<many-to-one column="sysroleid" name="sysRole"
			class="cn.com.icore.user.model.SysRole" />



		<subclass name="cn.com.icore.user.model.SysUserToSysRole" discriminator-value="U">
			<many-to-one column="sysuserid" name="sysUser"
				class="cn.com.icore.user.model.SysUser" />


		</subclass>


		<subclass name="cn.com.icore.user.model.SysGroupToSysRole" discriminator-value="G">
			<many-to-one column="sysgroupid" name="sysGroup"
				class="cn.com.icore.user.model.SysGroup" />
		</subclass>

	</class>
</hibernate-mapping>
