<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.mpaike.image.model.Picture" table="paike_picture">

		<id name="id" type="java.lang.Long" column="pictureid">
			<generator class="assigned" />
		</id>
		<property name="hotNum" column="hotNum" type="java.lang.Long" />
		
		<property name="resourceType" column="resourceType" type="java.lang.Long" />
		
		<property name="userId" column="userId" type="java.lang.Long"
			not-null="true" />
		<property name="userId" column="userId" type="java.lang.Long"
			not-null="true" />


		<property name="sourceName" column="sourceName" type="string" />
		
		<property name="gene" column="gene" type="string" length="1024"/>

		<property name="filename" column="filename" type="string" />


		<property name="fileType" column="fileType" type="string" />



		<property name="mimeType" column="mimeType" type="string" />


		<property name="fileSize" column="fileSize" type="string" />


		<property name="fileDataTime" column="fileDataTime" type="string" />


		<property name="srcHeight" column="srcHeight" type="java.lang.Integer"
			not-null="true" />



		<property name="srcWidth" column="srcWidth" type="java.lang.Integer" />


		<property name="imageDescription" column="imageDescription"
			type="string" />



		<property name="make" column="make" type="string" />


		<property name="model" column="model" type="string" />


		<property name="orientation" column="orientation" type="string" />


		<property name="xResolution" column="xResolution" type="string" />


		<property name="yResolution" column="yResolution" type="string" />


		<property name="software" column="software" type="string" />


		<property name="dateTime" column="dateTime" type="string" />


		<property name="artist" column="artist" type="string" />


		<property name="yCbCrPositioning" column="yCbCrPositioning"
			type="string" />


		<property name="copyright" column="copyright" type="string" />


		<property name="copyrightOfPhotographer" column="copyrightOfPhotographer"
			type="string" />


		<property name="copyrightOfEditor" column="copyrightOfEditor"
			type="string" />


		<property name="exifVersion" column="exifVersion" type="string" />


		<property name="flashPixVersion" column="flashPixVersion"
			type="string" />



		<property name="dateTimeDigitized" column="dateTimeDigitized"
			type="string" />


		<property name="computedHeight" column="computedHeight" type="string" />


		<property name="computedWidth" column="computedWidth" type="string" />


		<property name="apertureValue" column="apertureValue" type="string" />


		<property name="shutterSpeedValue" column="shutterSpeedValue"
			type="string" />


		<property name="apertureFNumber" column="apertureFNumber"
			type="string" />


		<property name="maxApertureValue" column="maxApertureValue"
			type="string" />


		<property name="exposureTime" column="exposureTime" type="string" />


		<property name="fNumber" column="fNumber" type="string" />


		<property name="meteringMode" column="meteringMode" type="string" />


		<property name="lightSource" column="lightSource" type="string" />


		<property name="flash" column="flash" type="string" />


		<property name="exposureMode" column="exposureMode" type="string" />


		<property name="whiteBalance" column="whiteBalance" type="string" />


		<property name="exposureProgram" column="exposureProgram"
			type="string" />


		<property name="exposureBiasValue" column="exposureBiasValue"
			type="string" />


		<property name="isoSpeedRatings" column="isoSpeedRatings"
			type="string" />


		<property name="componentsConfiguration" column="componentsConfiguration"
			type="string" />


		<property name="compressedBitsPerPixel" column="compressedBitsPerPixel"
			type="string" />


		<property name="focusDistance" column="focusDistance" type="string" />


		<property name="focalLength" column="focalLength" type="string" />


		<property name="focalLengthIn35mmFilm" column="focalLengthIn35mmFilm"
			type="string" />


		<property name="userCommentEncoding" column="userCommentEncoding"
			type="string" />


		<property name="userComment" column="userComment" type="string" />


		<property name="colorSpace" column="colorSpace" type="string" />


		<property name="exifImageLength" column="exifImageLength"
			type="string" />


		<property name="exifImageWidth" column="exifImageWidth" type="string" />


		<property name="fileSource" column="fileSource" type="string" />


		<property name="sceneType" column="sceneType" type="string" />


		<property name="thumbnailFileType" column="thumbnailFileType"
			type="string" />


		<property name="thumbnailMimeType" column="thumbnailMimeType"
			type="string" />



	</class>
</hibernate-mapping>
