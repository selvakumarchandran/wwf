<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE hibernate-mapping SYSTEM
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.mpaike.affix.model">
	 
	<class name="Affix" table="icore_affix" >
		<id name="id" type="java.lang.Long" column="ID"
			unsaved-value="null">
			<generator
				class="native"> 
				<param name="sequence">icore_affix_seq</param>
			</generator>
		</id>

		<property name="name" column="name" type="java.lang.String"
			length="200"  >
		</property>
		<property name="source" column="source"
			type="java.lang.String" length="255"  >
		</property>
		<property name="type" column="type"
			type="java.lang.String" length="200"  >
		</property>
		<property name="size" column="f_size"
			type="java.lang.String" length="10"  >
		</property>
		<property name="extname" column="extname"
			type="java.lang.String" length="10"  >
		</property>
		<property name="objectId" column="objectId"
			type="java.lang.String" length="50"  >
		</property>
		
		<property name="createDate" column="createDate"
			type="java.util.Date"   >
		</property>
		
		<property name="objectType" column="objectType"
			type="java.lang.Integer">
		</property>
 		<property name="hotNum" column="hotNum" type="java.lang.Long">
		</property>

	</class>
	 
</hibernate-mapping>