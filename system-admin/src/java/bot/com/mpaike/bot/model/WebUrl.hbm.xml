<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.mpaike.bot.model.WebUrl" table="paike_weburl">

		<id name="id" type="java.lang.Long" column="userid">
			<generator class="assigned" />
		</id>
		<property name="siteName" column="siteName" type="string"
			not-null="true">
		</property>
		<property name="url" column="url" type="string"
			not-null="true">

		</property>
		<property name="enName" column="enName" type="string">

		</property>
		<property name="threadNum" column="threadNum" type="java.lang.Integer">

		</property>

		<property name="status" column="status" type="java.lang.Integer">

		</property>
		<property name="width" column="width" type="java.lang.Integer">

		</property>
		<property name="height" column="height" type="java.lang.Integer">

		</property>
	</class>
</hibernate-mapping>
