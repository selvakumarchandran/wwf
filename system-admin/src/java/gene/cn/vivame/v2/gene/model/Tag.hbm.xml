<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="cn.vivame.v2.gene.model.Tag" table="v2_t_tag" >
        <id name="name" type="java.lang.String" column="name">
            <generator class="assigned" />
        </id>
        <property name="type" type="java.lang.Integer">
            <column name="type" />
        </property>
        <property name="semProperty" type="java.lang.Integer">
            <column name="semproperty" />
        </property>
        <property name="tagSelected" type="java.lang.Integer">
            <column name="tagselected" />
        </property>
        <property name="commend" type="java.lang.Integer">
            <column name="commend" />
        </property>
        <property name="tagModel" type="java.lang.Integer">
            <column name="tagModel" />
        </property>
    </class>
</hibernate-mapping>
