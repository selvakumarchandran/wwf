<?xml version="1.0" encoding="UTF-8"?>
<beans
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">
	
	<bean name="webUrlDao" class="com.mpaike.bot.dao.impl.WebUrlDao">
		<property name="sessionFactory" ref="sessionFactory" />
	</bean>
	
	<bean name="baseService" class="com.mpaike.sys.service.impl.BaseServiceImpl"
		abstract="true">
		<property name="webUrlDao" ref="webUrlDao"/>
		<property name="memberDao" ref="memberDao"/>
	</bean>
	
	<bean id="webUrlService"
		class="com.mpaike.bot.service.impl.WebUrlService"
		autowire="byName" parent="baseService">
		
	</bean>
	
	<bean name="memberDao" class="com.mpaike.member.dao.impl.MemberDao">
		<property name="sessionFactory" ref="sessionFactory" />
	</bean>
	<bean id="memberService"
		class="com.mpaike.member.service.impl.MemberServiceImpl"
		autowire="byName" parent="baseService"/>
</beans>