<struts>
	<package name="spider" extends="default" >

		<action name="spiderAction" class="com.mpaike.bot.action.SpiderAction">
			<result name="start_spider" type="dispatcher">
				<param name="location">/manager/bot/spider_run.jsp</param>
			</result>
			<result name="ajax_content" type="dispatcher">
