<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="picture" extends="default" >

		
		<action name="pictureAction"
			class="com.mpaike.image.action.PictureAction">
			<result name="imageList" type="dispatcher">
				<param name="location">/manager/bot/picture_imageview.jsp</param>
			</result>
			<result name="imageProcess" type="dispatcher">
				<param name="location">/manager/bot/picture_imageprocess.jsp</param>
			</result>
		</action>
	</package>
</struts>
